spring.application.name=gerencimentotarefas

spring.datasource.url=jdbc:postgresql://localhost:5432/gerenciamentotarefas
spring.datasource.username=postgres
spring.datasource.password=7783
spring.datasource.driver-class-name=org.postgresql.Driver

spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.PostgreSQLDialect
spring.jpa.hibernate.ddl-auto=none
spring.jpa.hibernate.packagesToScan=com.renatachom.gerencimentotarefas.entity

server.port=8081

spring.mvc.pathmatch.matching-strategy=ant_path_matcher

#LOG
logging.level.root=INFO
logging.level.com.renatachom.gerencimentotarefas=DEBUG
logging.file=logs/gerencimentotarefas.log

log4j.rootLogger=INFO, stdout

log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss} %-5p %c{1}:%L - %m%n


# Configurações do Flyway
spring.flyway.url=jdbc:postgresql://localhost:5432/meu_banco_de_dados
spring.flyway.user=meu_usuario
spring.flyway.password=minha_senha
spring.flyway.locations=classpath:db/migration





